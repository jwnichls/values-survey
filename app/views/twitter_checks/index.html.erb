<h1>Listing twitter_checks</h1>

<table>
  <tr>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @twitter_checks.each do |twitter_check| %>
  <tr>
    <td><%= link_to 'Show', twitter_check %></td>
    <td><%= link_to 'Edit', edit_twitter_check_path(twitter_check) %></td>
    <td><%= link_to 'Destroy', twitter_check, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
